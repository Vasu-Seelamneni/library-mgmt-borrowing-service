CREATE TABLE borrowings (
                            id SERIAL PRIMARY KEY,
                            user_id INT,
                            book_id INT,
                            borrowed_date TIMESTAMP,
                            due_date TIMESTAMP,
                            returned_date TIMESTAMP
);

